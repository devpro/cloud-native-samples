{
  "AllowedHosts": "*",
  "AllowedOrigins": [
  ],
  "Application": {
    "IsHttpsRedirectionEnabled": "true",
    "IsOpenTelemetryEnabled": "false",
    "IsSwaggerEnabled": "false"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "SplunkOpenTelemetrySample": "Information"
    }
  },
  "OpenApi": {
    "Title": "Splunk OpenTelemetry Sample WebApi",
    "Version": "v1.0"
  },
  "OpenTelemetry": {
    "ServiceName": "otel",
    "CollectorEndpoint": "http://localhost:4317"
  }
}
