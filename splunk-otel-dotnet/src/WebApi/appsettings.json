{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Application": {
    "Name": "Splunk OpenTelemetry Sample WebApi",
    "Version": "v1",
    "EnforceHttps":  "true"
  },
  "OpenTelemetryTracing": {
    "Enabled" : "false",
    "Service": "otel",
    "OtlpExporter": {
      "Endpoint": "http://localhost:4317"
    }
  }
}
